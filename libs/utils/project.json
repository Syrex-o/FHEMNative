    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/utils/src/**/*.ts",
          "libs/utils/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
